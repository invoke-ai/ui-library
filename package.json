{
  "name": "@invoke-ai/ui-library",
  "version": "0.0.47",
  "description": "UI Components for Invoke's applications.",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/invoke-ai/ui-library.git"
  },
  "author": "Invoke",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/invoke-ai/ui-library/issues"
  },
  "homepage": "https://github.com/invoke-ai/ui-library#readme",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-storybook": "storybook build",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepublishOnly": "pnpm run build",
    "storybook": "storybook dev -p 6006 --no-open",
    "typescript": "tsc --p ./tsconfig-build.json --noEmit",
    "test": "vitest",
    "test:ui": "vitest --coverage --ui",
    "test:no-watch": "vitest --no-watch"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.3.4",
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/layout": "^2.3.1",
    "@chakra-ui/portal": "^2.1.0",
    "@chakra-ui/react": "^2.10.9",
    "@chakra-ui/styled-system": "^2.12.0",
    "@chakra-ui/theme-tools": "^2.2.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@nanostores/react": "^1.0.0",
    "chakra-react-select": "^4.10.1",
    "es-toolkit": "^1.39.10",
    "framer-motion": "^10.18.0",
    "math-expression-evaluator": "^2.0.7",
    "nanostores": "^1.0.1",
    "overlayscrollbars": "2.12.0",
    "overlayscrollbars-react": "^0.5.6",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.2"
  },
  "peerDependencies": {
    "@fontsource-variable/inter": "^5.0.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@storybook/addon-links": "^9.1.4",
    "@storybook/addon-onboarding": "^9.1.4",
    "@storybook/react": "^9.1.4",
    "@storybook/react-vite": "^9.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-path": "^2.0.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.1.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,yaml}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
